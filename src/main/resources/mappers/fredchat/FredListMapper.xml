<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.multi.gamegather.fredchat.model.dao.FredListDAO">

    <!-- 결과 매핑 -->
    <resultMap id="fredListResultMap" type="com.multi.gamegather.fredchat.model.dto.FredListDTO">
        <id property="no" column="NO" />
        <result property="frndCode" column="FRND_CODE" />
        <result property="userId" column="USER_ID" />
        <result property="frndId" column="FRND_ID" />
        <result property="frndFlag" column="FRND_FLAG" />
    </resultMap>

    <select id="selectFrndCodesByUserId" parameterType="String" resultType="String">
        SELECT FRND_CODE
        FROM TB_FRED_LIST
        WHERE USER_ID = #{userId}
    </select>
</mapper>